msgstr "Creators"

msgid "plugins.generic.pidManager.igsn.workflow.table.publisher"
msgstr "Publisher"

msgid "plugins.generic.pidManager.igsn.workflow.table.publicationYear"
msgstr "Year"

msgid "plugins.generic.pidManager.igsn.button.add"
msgstr "Add new row"

msgid "plugins.generic.pidManager.igsn.remove.confirm"
msgstr "Are you sure you want to remove?"

msgid "plugins.generic.pidManager.igsn.datacite.searchPhraseDoi.placeholder"
msgstr "Type your DOI here, e.g. 10.12345/12.3456"

msgid "plugins.generic.pidManager.igsn.datacite.searchPhraseLabel.placeholder"
msgstr "Type your label here, e.g. Sediment core"

msgid "plugins.generic.pidManager.igsn.datacite.info"
msgstr "Please type your search phrase above and search on DataCite."

msgid "plugins.generic.pidManager.igsn.datacite.empty"
msgstr "Your search phrase returned empty."


